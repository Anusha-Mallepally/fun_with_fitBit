        <operation_script><![CDATA[(function process(/*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {
	//var json = new global.JSON();
	var loginfo = "get Referer";
	var headers = request.headers;

	var refererVal = headers.referer;
	loginfo += "\nreferer:" + refererVal;
	var refererID = refererVal.substr(refererVal.indexOf("sys_id=")+7,32);
	loginfo += "\nID:" + refererID;
	var queryParams = request.queryParams;
	var code = queryParams.code;
	loginfo += "\ncode:" + code;
	gs.info(loginfo,"Oauth Info");
	var  tkUtil = new x_fun_with_fitbit.FitBitOathHandler("fitbitOauthGetToken",refererID);
	return   tkUtil.getToken(code);

})(request, response);]]></operation_script>
